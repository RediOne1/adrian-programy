    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.projekt.bazydanych.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.projekt.bazydanych.Rejestracja"
            android:label="@string/title_activity_rejestracja" >
        </activity>
        <activity
            android:name="com.projekt.bazydanych.Login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.projekt.bazydanych.WybierzTyp"
            android:label="@string/title_activity_wybierz_typ" >
        </activity>
        <activity
            android:name="com.projekt.bazydanych.PokazTyp"
            android:label="@string/title_activity_pokaz_typ" >
        </activity>
        <activity
            android:name="com.projekt.bazydanych.Produkt"
            android:label="@string/title_activity_produkt" >
        </activity>
        <activity
            android:name="com.projekt.bazydanych.Zamowienie"
            android:label="@string/title_activity_zamowienie" >
        </activity>
    </application>

</manifest>
